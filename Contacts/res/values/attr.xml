<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ChangeColorIconWithText">
        <attr name="icon" format="reference|color"></attr>
        <attr name="color" format="color"></attr>
        <attr name="text" format="string"></attr>
        <attr name="text_size" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="TextViewWithTitle">
        <attr name="title" format="string"></attr>
        <attr name="title_size" format="dimension"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>
    <declare-styleable name="EditTextWithTitle">
        <attr name="title"></attr>
        <attr name="title_size"></attr>
        <attr name="text"></attr>
        <attr name="text_size"></attr>
    </declare-styleable>

</resources>